<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <!-- Package information -->
    <id>ManifestPackage</id>
    <version>1.0.0</version>
    <authors>author</authors>
    <owners>owner</owners>
    <description>A NuGet package that holds the manifests for the {{WORKLOAD_NAME}} workload.</description>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
  </metadata>
  <files>
    <!-- Include the XML and FE files -->
    <file src="<ManifestFolder>*.xml" target="BE" />
    <file src="<ManifestFolder>*.json" target="FE" />
    <file src="<ManifestFolder>assets\**" target="FE\assets" exclude="<ManifestFolder>assets\items\**" />
    <file src="<ManifestFolder>assets\items\**" target="Assets\items" />
  </files>
</package>
