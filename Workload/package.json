{
  "name": "fabric-developer-sample",
  "version": "2.0.0",
  "description": "Microsoft Fabric - Developer Sample App",
  "scripts": {    
    "start": "npm run start:devServer",
    "start:devServer": "env-cmd -f .env.dev webpack serve --open --config ./devServer/webpack.config.js",
    "start:devGateway": "node ./devServer/start-devGateway.js",
    "start:codespace": "node --max-old-space-size=4096 ./node_modules/.bin/env-cmd -f .env.dev webpack serve --open --config ./devServer/webpack.config.js --no-hot",    
    "build:test": "env-cmd -f .env.test webpack --config ./devServer/webpack.config.js --output-path ./dist --progress",
    "build:prod": "env-cmd -f .env.prod webpack --config ./devServer/webpack.config.js --output-path ./dist --mode production --progress"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fluentui/react": "^8.110.7",
    "@fluentui/react-components": "^9.7.2",
    "@fluentui/react-icons": "2.0.226",
    "@ms-fabric/workload-client": "^2.3.0",
    "@reduxjs/toolkit": "^2.6.0",
    "history": "^4.9.0",
    "i18next": "^23.12.3",
    "i18next-http-backend": "^2.4.2",
    "jwt-decode": "^3.1.2",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-i18next": "^15.0.1",
    "react-redux": "^9.2.0",
    "react-router-dom": "^5.3.4",
    "uuid": "^11.0.5"
  },
  "devDependencies": {
    "@types/node": "^16.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/react-router-dom": "^5.3.2",
    "typescript": "^5.0.4",
    "ajv": "^8.12.0",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^10.0.0",
    "css-loader": "^6.5.1",
    "dotenv": "^16.4.5",
    "env-cmd": "^10.1.0",
    "html-webpack-plugin": "^5.5.0",
    "nuget-bin": "^4.0.0",
    "sass": "^1.79.3",
    "sass-loader": "^16.0.2",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.2.6",
    "webpack": "^5.90.3",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.7.0"
  }
}