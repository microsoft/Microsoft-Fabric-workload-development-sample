    "/resolve-api-path-placeholder": {
      "post": {
        "summary": "Resolve an endpoint for a given service called by Microsoft Fabric",
        "description": "Resolves the endpoint for a given service called by Microsoft Fabric based on the tenant's region and workspace region. \n\nThis API should accept [SubjectAndApp authentication](https://subjectandappauthentication). However, the subject token should never be present.\n\n## Permissions\n\nPermissions are checked by Microsoft Fabric.",
        "operationId": "EndpointResolution_Resolve",
        "tags": [ "EndpointResolution" ],
        "consumes": [ "application/json" ],
        "produces": [ "application/json" ],
        "parameters": [
